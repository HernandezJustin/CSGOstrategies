<!-- Filter wrapper -->
<div class="row filter-wrapper visible-box" id="filter-box">
  <div class="col-lg-12">
    <div class="filter-header">
      <button aria-label="Close" class="close toggle-filter" type="button" data-block-id="filter-box"><i class="icon-cancel"></i></button>
      <h3 class="title">Generate A Strat</h3>
    </div>
    <form class="form-inline">
      <div class="form-group">
        <label class="form-label">Map</label>
        <select class="form-control" id="map_names" ng-model="maps" ng-change="repopulateStrats(maps)" ng-init="maps='Dust 2'" disabled="true">
          <option ng-repeat="mapName in mapNames" value="{{mapName}}">{{mapName}}</option>
        </select>
        <!-- <input type="text" placeholder="Separate by commas..." class="form-control"> -->
      </div>
      <div class="form-group">
        <label class="form-label">Team</label>
        <select class="form-control" id="team_names" ng-model="teams" ng-change="repopulateStrats(teams)" ng-init="teams='T'" disabled="true">
          <option ng-repeat="teamName in teamNames" value="{{teamName}}">{{teamName}}</option>
        </select>
      </div>
      <div class="form-group">
        <div class="checkbox-group">
          <div class="checkbox checkbox-replace">
            <input type="checkbox" id="onEco">
            <label for="onEco">On Eco</label>
          </div>
        </div>
      </div>
      <div class="form-group filter-btn">
        <button class="btn btn-default" ng-click="findStrat(strat)" disabled="true" id="generate_btn">Generate</button>
      </div>
    </form>
  </div>
</div>
</div>
</div>
<!-- /filter wrapper -->
